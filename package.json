  "description": "Object-Oriented REST API framework",
  "main": "lib/api.js",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": "src/**/*.ts",
    "extension": [
      ".ts"
    ]
  },
  "scripts": {
