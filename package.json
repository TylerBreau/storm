{
  "name": "@breautek/storm",
  "version": "1.0.0",
  "description": "Object-Oriented REST API framework",
  "main": "lib/api.js",
  "types": "lib/api.d.ts",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "include": "src/**/*.ts",
    "extension": [
      ".ts"
    ],
    "lines": 80,
    "watermarks": {
      "lines": [
        80,
        90
      ]
    },
    "exclude": [
      "**/*Middleware*"
    ]
  },
  "scripts": {
    "build": "tsc && npm run-script build-docs",
    "build-lib": "rm -r ./lib && tsc",
    "build-docs": "typedoc --excludePrivate --excludeNotExported --mode file -readme ./README.md --out ./docs ./src",
    "lint": "eslint --ext .ts '?(src|spec)/**/*.ts'",
    "unit": "ts-node ./spec/support/jasmine.ts",
    "test": "npm run-script lint && nyc --reporter=lcov --reporter=text ts-node ./spec/support/jasmine.ts",
    "coverage": "codecov",
    "clean": "rm -r ./lib && rm -r ./docs",
    "prepublishOnly": "npm run-script build-lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/breautek/storm.git"
  },
  "keywords": [
    "web",
    "api",
    "storm",
    "rest",
    "object",
    "oriented",
    "breautek"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "norman@normanbreau.com",
  "license": "GPL-3.0",
  "homepage": "https://github.com/breautek/storm.git",
  "dependencies": {
    "@breautek/iterator": "^1.0.2",
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.0",
    "@types/formidable": "1.0.31",
    "@types/jsonwebtoken": "8.3.4",
    "@types/mysql": "^2.15.6",
    "@types/node": "^12.7.11",
    "@types/uuid": "^3.4.4",
    "body-parser": "^1.19.0",
    "commander": "^2.20.0",
    "express": "^4.17.1",
    "formidable": "1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.17.1",
    "source-map-support": "^0.5.12",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@breautek/eslint-plugin": "3.0.0",
    "@types/jasmine": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
    "codecov": "3.6.1",
    "eslint": "^6.5.1",
    "form-data": "^2.5.0",
    "jasmine": "3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nyc": "^14.1.1",
    "ts-node": "8.4.1",
    "tsutils": "^3.14.0",
    "typedoc": "^0.15.0",
    "typescript": "3.6.3"
  }
}
