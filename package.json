  "dependencies": {
    "@types/body-parser": "1.17.0",
    "@types/express": "^4.16.1",
    "@types/formidable": "1.0.31",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mysql": "2.15.5",
    "@types/node": "^10.12.18",
    "@types/uuid": "^3.4.4",
    "body-parser": "1.18.3",
    "commander": "^2.20.0",
    "express": "^4.16.4",
    "formidable": "1.2.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "2.16.0",
    "tslib": "^1.9.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@types/jasmine": "^3.3.9",
    "jasmine": "^3.3.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.3.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "nyc": "^13.3.0",
    "ts-node": "^8.0.2",
    "tslint": "^5.15.0",
    "tsutils": "^3.10.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.4.3"
  }
